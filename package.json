{
  "name": "dvy",
  "version": "1.0.0",
  "description": "A decentralised video library platform, built on IPFS.",
  "scripts": {
    "tsc": "tsc",
    "run": "electron out/index.js",
    "parcel:dev": "parcel src/react/index.html --port 3001",
    "parcel:build": "parcel build src/react/index.html --public-url ./ --dist-dir out/react",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "build": "npm run lint && npm run tsc",
    "start": "npm run build && npm run run",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run parcel:dev\" \"npm start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leahlundqvist/dvy.git"
  },
  "keywords": [
    "ipfs"
  ],
  "author": "Leah Lundqvist",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leahlundqvist/dvy/issues"
  },
  "homepage": "https://github.com/leahlundqvist/dvy#readme",
  "dependencies": {
    "electron": "^19.0.1",
    "ipfs": "^0.62.3",
    "module-alias": "^2.2.2",
    "ololog": "^1.1.175",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "^2.6.0",
    "@types/node": "^17.0.36",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "parcel": "^2.6.0",
    "postcss": "^8.4.14",
    "process": "^0.11.10",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.2"
  }
}
